{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["src"], 
      "exclude": ["node_modules", "test"],
      "includePattern": ".js$"
    },
    "opts": {
      "destination": "./doc", 
      "recurse": true,
      "readme": "./doc/api/api.yaml"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }
  